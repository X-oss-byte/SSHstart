# Normal
Host a

# Repeat
Host a

# Tab before
	Host b

# Spaces before
  Host c

# Spaces after
Host d  

# Multiple hosts
Host ee   f

# Different key
HostName g

# Empty
Host

# Empty with space
Host 

# Host followed by tab
Host	h

# Host followed multiple spaces
Host    i

# Just spaces
  

# Unicode
Host 验🍍

# Equal sign
Host = j

# Quotes
Host = "k" "l l"

# Case-insensitive
HoST m

# Just quotes
Host ""

# Short key
Ho n